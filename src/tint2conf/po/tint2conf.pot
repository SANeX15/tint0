msgstr ""

#: ../properties.c:1067
msgid "Show panel after"
msgstr ""

#: ../properties.c:1077
msgid ""
"Specifies a delay after which the panel is shown when the mouse cursor "
"enters the panel."
msgstr ""

#: ../properties.c:1079 ../properties.c:1113 ../properties.c:3903
#: ../properties.c:3922
msgid "seconds"
msgstr ""

#: ../properties.c:1087
msgid "Hidden size"
